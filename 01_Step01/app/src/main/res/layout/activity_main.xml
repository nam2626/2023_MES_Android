<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <Button
        android:id="@+id/btn_none"
        android:layout_width="187dp"
        android:layout_height="70dp"
        android:layout_marginStart="120dp"
        android:layout_marginTop="132dp"
        android:backgroundTint="#060D53"
        android:onClick="onButtonClick"
        android:text="버튼"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:strokeColor="#C31E1E"
        app:strokeWidth="10dp" />

    <Button
        android:id="@+id/btn_naver"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="48dp"
        android:layout_marginTop="316dp"
        android:onClick="onNaverButton"
        android:text="네이버 접속"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_none" />

    <Button
        android:id="@+id/btn_tel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="76dp"
        android:layout_marginTop="316dp"
        android:text="전화걸기"
        app:layout_constraintStart_toEndOf="@+id/btn_naver"
        app:layout_constraintTop_toBottomOf="@+id/btn_none" />
</androidx.constraintlayout.widget.ConstraintLayout>